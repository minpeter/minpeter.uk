{
  "redirects": [
    {
      "source": "/:path",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-host",
          "value": "blog.minpeter.xyz"
        }
      ],
      "destination": "https://minpeter.xyz/blog/:path"
    },
    {
      "source": "/",
      "has": [
        {
          "type": "header",
          "key": "x-forwarded-host",
          "value": "blog.minpeter.xyz"
        }
      ],
      "destination": "https://minpeter.xyz/blog/"
    }
  ]
}
